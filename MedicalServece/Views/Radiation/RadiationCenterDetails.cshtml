@model MedicalServece.Models.RadiationLab

@{
    ViewBag.Title = "RadiationCenterDetails";
    Layout = "~/Views/Shared/_Manage.cshtml";
}

<h2>RadiationCenterDetails</h2>

<div>
    <h4>RadiationLab</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.R_Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.R_Name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.R_Phone)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.R_Phone)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.R_Address)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.R_Address)
        </dd>
        @foreach (var item in Model.radiationLabContent)
